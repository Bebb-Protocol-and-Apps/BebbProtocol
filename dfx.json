{
  "canisters": {
    "bebb": {
      "main": "src/bebb/main.mo",
      "type": "motoko"
    },
    "index": {
      "type": "motoko",
      "main": "src/index/IndexCanister.mo",
      "declarations": {
        "node_compatibility": true
      }
    },
    "helloservice": {
      "type": "motoko",
      "main": "src/helloservice/HelloService.mo",
      "declarations": {
        "node_compatibility": true
      }
    }
  },
  "declarations": {
    "output": "src/declarations"
  },
  "defaults": {
    "build": {
      "packtool": "mops sources"
    }
  },
  "networks": {
    "development": {
      "providers": [
        "https://icp0.io"
      ],
      "type": "persistent"
    },
    "testing": {
      "providers": [
        "https://icp0.io"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}