{
  "canisters": {
    "index": {
      "type": "motoko",
      "main": "src/bebb/can_db/index/IndexCanister.mo",
      "declarations": {
        "node_compatibility": true
      }
    },
    "frontend": {
      "dependencies": [
        "index"
      ],
      "frontend": {
        "entrypoint": "dist/index.html"
      },
      "source": [
        "dist/"
      ],
      "type": "assets"
    }
  },
  "declarations": {
    "output": "src/declarations"
  },
  "defaults": {
    "build": {
      "packtool": "mops sources"
    }
  },
  "networks": {
    "development": {
      "providers": [
        "https://icp0.io"
      ],
      "type": "persistent"
    },
    "testing": {
      "providers": [
        "https://icp0.io"
      ],
      "type": "persistent"
    },
    "alexStaging": {
      "providers": [
        "https://icp0.io"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}
